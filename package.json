{
  "name": "bobjan",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:background": "ng serve & $(npm bin)/wait-on -v http://localhost:4200",
    "build": "ng build --prod",
    "test": "jest",
    "test:coveralls": "cat ./coverage/lcov.info | $(npm bin)/coveralls",
    "test:watch": "jest --watch",
    "test:cc": "jest --coverage",
    "lint": "ng lint",
    "cypress": "$(npm bin)/cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "ng build --prod && run-p --race start:ci cypress:run",
    "start:ci": "$(npm bin)/http-server ./dist/bobjan -p 4200 -c-1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/router": "~7.2.15",
    "core-js": "~2.5.4",
    "rxjs": "~6.5.1",
    "tslib": "~1.9.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.9",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.15",
    "@angular/language-service": "~7.2.15",
    "@types/jest": "^24.0.12",
    "@types/node": "~12.0.0",
    "codelyzer": "~5.0.1",
    "coveralls": "^3.0.3",
    "cypress": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.0",
    "netlify-cli": "^2.11.16",
    "npm-run-all": "^4.1.5",
    "transformation-matrix": "^2.0.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "typescript": "~3.2.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.conf.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress/"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": ["lcov"]
  }
}
